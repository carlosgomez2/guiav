<div class="search-box">
	<form class="form-inline search" role="form">
	  <div class="form-group" >
	    <input type="text" class="form-control" style="width:600px;" id="exampleInputEmail2" placeholder="What are you looking for?">
	  </div>
	  <div class="form-group">
	    <button type="submit" class="btn btn-primary" style=" font-weight: bold;">Search</button>
	  </div>
	 </form>
</div>

<div class="search-results">
	<div class="row">
	 	<div class="col-md-3">
	 		<div class="panel panel-primary filter-panel">
	 			<div class="panel-heading">
				    <h4 class="panel-title">Laboratorios</h3>
				  </div>
		 		<ul>
		 			<li><input type="checkbox" value="Bike">Lab1</li>
		 			<li><input type="checkbox" value="Bike">Lab2</li>
		 			<li><input type="checkbox" value="Bike">Lab3</li>
		 			<li><input type="checkbox" value="Bike">Lab4</li>
		 			<li><input type="checkbox" value="Bike">Lab5</li>
		 		</ul>
	 		</div>
	 		
	 	</div>
	 	<div class="col-md-9">
	 		<div id="table">
				<table class="table table-striped">
					<thead>
						<tr>
							<th>Nombre</th>
							<th>Laboratorio</th>
							<th>Tipo</th>
							<th>Categoria</th>
							<th>Especies</th>
							<th></th>
						</tr>
					</thead>
					<tbody>
						<% Medicamento.all.each do |medicamento| %>
							<tr>
								<td><%= medicamento.nombre %></td>
								<td><%= medicamento.laboratorio.nombre %></td>
								<td></td>
								<td></td>
								<td>
									<ul>
										<% medicamento.animals.each do |animal| %>
											<li class="animals"><%= image_tag "#{animal.nombre.downcase}.png" , height: '25',title: "Bovinos" %></li>
										<% end %>
									</ul>
								 </td>
								<td><a href="">Ver mas</a></td>
								
							</tr>
						<% end %>
					</tbody>
				</table>
			</div>
	 	</div>
	 </div>
</div>
 
